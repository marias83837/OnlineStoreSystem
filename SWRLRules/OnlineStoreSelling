Individual(?x) ^ hasBelief(?x, ?belief) -> Belief(?x, ?belief)
Belief(?x, ?belief1) ^ supports(?belief1, ?belief2) ^ not(Belief(?x, ?belief2)) -> Belief(?x, ?belief2)
Belief(?x, ?belief1) ^ Belief(?x, ?belief2) ^ contradicts(?belief1, ?belief2) -> Contradiction(?x, ?belief1, ?belief2)
hasBelief(PeerA, PositiveReviews(Product)) ^ supports(PositiveReviews(Product), HighQuality(Product)) -> hasBelief(PeerA, HighQuality(Product))
hasBelief(PeerB, FrequentlyPurchased(Product)) ^ supports(FrequentlyPurchased(Product), Popular(Product)) -> hasBelief(PeerB, Popular(Product))
hasBelief(PeerA, HighQuality(Product)) -> hasBelief(PeerB, HighQuality(Product))
hasBelief(PeerA, FrequentlyPurchased(Product)) ^ supports(FrequentlyPurchased(Product), Popular(Product)) -> hasBelief(PeerA, Popular(Product))
hasBelief(PeerB, PositiveReviews(Product)) ^ supports(PositiveReviews(Product), HighQuality(Product)) -> hasBelief(PeerB, HighQuality(Product))
